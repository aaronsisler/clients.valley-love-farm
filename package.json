{
  "name": "valley-love-farm",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf ./build && rm -rf ./dist",
    "build": "next build ./src && next export -o dist ./src",
    "dev": "next ./src",
    "deploy:beta": "aws s3 sync ./dist s3://beta.valleylovefarm.com --delete && aws cloudfront create-invalidation --distribution-id CHANGE_ME --paths \"/*\"",
    "deploy:prod": "aws s3 sync ./dist s3://valleylovefarm.com --delete && aws cloudfront create-invalidation --distribution-id CHANGE_ME --paths \"/*\"",
    "lint": "eslint \"./**/*.{js,jsx}\"",
    "gen:comp": "node ./node_modules/@ebsolutions/generate-react-templates --type=COMP",
    "gen:cont": "node ./node_modules/@ebsolutions/generate-react-templates --type=CONT",
    "gen:page": "node ./node_modules/@ebsolutions/generate-react-templates --type=PAGE",
    "sb": "start-storybook -p 3000",
    "build-sb": "build-storybook"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@hookform/resolvers": "2.8.3",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.8",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "email-validator": "^2.0.4",
    "next": "^12.0.10",
    "prop-types": "^15.7.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.20.2",
    "react-tabs": "^5.1.0",
    "sass": "^1.43.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@ebsolutions/generate-react-templates": "^1.7.0",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.4",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "stylelint": "^14.1.0",
    "stylelint-config-rational-order-fix": "^0.1.2",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0"
  }
}
